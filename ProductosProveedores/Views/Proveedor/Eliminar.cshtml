@model Proveedor
@{
    ViewData["Title"] = "Eliminar";
}

<h1>Eliminar</h1>

<div>
    <h3> Está usted seguro de querer eliminar el producto con el ID @Model.ID </h3>

    <form asp-controller="Home" asp-action="ManageProveedor" method="post">
        <input type="hidden" value="@Model.ID" />

        <button class="btn btn-danger" type="submit"> Aceptar</button>
        <button class="btn btn-info" asp-controller="Home" asp-action="ManageProveedor"> Volver </button>
    </form>

</div>
